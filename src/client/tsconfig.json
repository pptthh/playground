{
  "compilerOptions": {
    "module": "amd",
    "allowJs": true,
    "maxNodeModuleJsDepth": 1,
    "listEmittedFiles": true,
    "listFiles": true,
    "moduleResolution": "node",
    "noImplicitAny": true,
    //"outFile": "../../lib/client/clientBundle.js",
    "outDir": "../../lib/client",
    "preserveConstEnums": true,
    "removeComments": true,
    "typeRoots": [
      "../../node_modules/@types"
    ],
    "pretty": true,
    "sourceMap": true,
    "sourceRoot": "../",
    "rootDirs": [
      "../../node_modules"
    ]
  },
  "include": [
    "**/*.ts"
  ],
  "exclude": [
   // "node_modules"
  ]
}
